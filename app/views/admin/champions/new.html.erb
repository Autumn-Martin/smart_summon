<%= form_for @champion, url: admin_champions_path do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :damage %>
  <%= f.text_field :damage %>
  <%= f.label :primary_class %>
  <%= f.text_field :primary_class %>
  <%= f.label :secondary_class  %>
  <%= f.text_field :secondary_class %>
  <%= f.submit %>
<% end %>
