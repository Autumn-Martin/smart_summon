<h1>Log In</h1>
<%= form_tag login_path do |f| %>
  <div>
    <%= f.label_tag :email %>
    <%= f.text_field_tag :email, autofocus: true %>
  </div>
  <div>
    <%= f.label_tag :password %>
    <%= f.password_field_tag :password %>
  </div>
  <div>
    <%= f.submit_tag "Log In" %>
  </div>
<% end %>
